@article{whittlesea_discrepancy-attribution_2001,
	title = {The discrepancy-attribution hypothesis: {II} {Expectation}, uncertainty, surprise, and feelings of familiarity},
	volume = {27},
	issn = {0278-7393},
	shorttitle = {The discrepancy-attribution hypothesis},
	url = {http://ez-proxy.brooklyn.cuny.edu:2048/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2000-14052-002&site=ehost-live},
	doi = {10.1037/0278-7393.27.1.14},
	abstract = {In the accompanying article (B. W. A. Whittlesea and L. D. Williams, see record [rid]2000-14052-001[/rid]), surprising violation of and expectation was observed to cause an illusion of familiarity. The authors interpreted that evidence as support for the discrepancy-attribution hypothesis. This article extended the scope of that hypothesis, investigating the consequences of surprising validation of expectations. Ss were shown recognition probes as completions of sentence stems. Their expectations were manipulated by presenting predictive, nonpredictive, and inconsistent stems. Predictive stems caused an illusion of familiarity, but only when the Ss also experienced uncertainty about the outcome. That is, as predicted by the discrepancy-attribution hypothesis, feelings of familiarity occurred only when processing of a recognition target caused surprise. The article provides a discussion of the ways in which a perception of discrepancy can come about, as well as the origin and nature of unconscious expectation. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {1},
	urldate = {2018-07-12},
	journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
	author = {Whittlesea, Bruce W. A. and Williams, Lisa D.},
	month = jan,
	year = {2001},
	keywords = {Adult, Attitude, Attribution, Cognitive Dissonance, college students, Cues, discrepancy-attribution hypothesis \& expectation \& uncertainty \& surprise \& feelings of familiarity for sentence stems, Expectations, Familiarity, Female, Humans, Judgment, Linguistics, Male, Models, Psychological, Probability, Recognition (Psychology), Sentence Completion Tests},
	pages = {14--33},
	file = {Whittlesea and Williams - 2001 - The discrepancy-attribution hypothesis II Expecta.pdf:files/10/Whittlesea and Williams - 2001 - The discrepancy-attribution hypothesis II Expecta.pdf:application/pdf},
}

@article{whittlesea_discrepancy-attribution_2001-1,
	title = {The discrepancy-attribution hypothesis: {I} {The} heuristic basis of feelings and familiarity},
	volume = {27},
	issn = {0278-7393},
	shorttitle = {The discrepancy-attribution hypothesis},
	url = {http://ez-proxy.brooklyn.cuny.edu:2048/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2000-14052-001&site=ehost-live},
	doi = {10.1037/0278-7393.27.1.3},
	abstract = {B. W. A. Whittlesea and D. L. Williams (see records [rid]1998-02991-002[/rid] and [rid]2000-03416-001[/rid]) proposed the discrepancy-attribution hypothesis to explain the source of feelings of familiarity. By that hypothesis, people chronically evaluate the coherence of their processing. When the quality of processing is perceived as being discrepant from that which could be expected, people engage in an attributional process; the feeling of familiarity occurs when perceived discrepancy is attributed to prior experience. In the present article, the authors provide convergent evidence for that hypothesis and show that it can also explain feelings of familiarity for nonlinguistic stimuli. They demonstrate that the perception of discrepancy is not automatic but instead depends critically on the attitude that people adopt toward their processing, given the task and context. The connection between the discrepancy-attribution hypothesis and the 'revelation effect' is also explored (e.g., D. L. Westerman and R. L. Greene, see record [rid]1996-05780-006[/rid]). (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {1},
	urldate = {2018-07-12},
	journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
	author = {Whittlesea, Bruce W. A. and Williams, Lisa D.},
	month = jan,
	year = {2001},
	keywords = {Adult, Attitude, Attribution, Cognitive Dissonance, college students, Cues, Familiarity, Female, Humans, Linguistics, Male, Models, Psychological, Recognition (Psychology), Auditory Stimulation, Cognitive Processes, Deja Vu, discrepancy-attribution hypothesis \& heuristic basis of feelings of familiarity for nonligusitic stimuli, Heuristics, Music},
	pages = {3--13},
	file = {Whittlesea and Williams - 2001 - The discrepancy-attribution hypothesis I The heur.pdf:files/9/Whittlesea and Williams - 2001 - The discrepancy-attribution hypothesis I The heur.pdf:application/pdf},
}

