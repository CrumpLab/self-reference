@article{cunningham_survival_2013,
	title = {Survival of the selfish: {Contrasting} self-referential and survival-based encoding},
	volume = {22},
	issn = {1053-8100},
	shorttitle = {Survival of the selfish},
	url = {https://www.sciencedirect.com/science/article/pii/S1053810012002395},
	doi = {10.1016/j.concog.2012.12.005},
	abstract = {Processing information in the context of personal survival scenarios elicits a memory advantage, relative to other rich encoding conditions such as self-referencing. However, previous research is unable to distinguish between the influence of survival and self-reference because personal survival is a self-referent encoding context. To resolve this issue, participants in the current study processed items in the context of their own survival and a familiar other person's survival, as well as in a semantic context. Recognition memory for the items revealed that personal survival elicited a memory advantage relative to semantic encoding, whereas other-survival did not. These findings reinforce suggestions that the survival effect is closely tied with self-referential encoding, ensuring that fitness information of potential importance to self is successfully retained in memory.},
	language = {en},
	number = {1},
	urldate = {2023-04-18},
	journal = {Consciousness and Cognition},
	author = {Cunningham, Sheila J. and Brady-Van den Bos, Mirjam and Gill, Lucy and Turk, David J.},
	month = mar,
	year = {2013},
	keywords = {Memory, Self, Self-reference effect, Fitness value, Survival},
	pages = {237--244},
	file = {ScienceDirect Full Text PDF:files/156/Cunningham et al. - 2013 - Survival of the selfish Contrasting self-referent.pdf:application/pdf;ScienceDirect Snapshot:files/157/S1053810012002395.html:text/html},
}

@article{grilli_self-imagination_2011,
	title = {The {Self}-{Imagination} {Effect}: {Benefits} of a {Self}-{Referential} {Encoding} {Strategy} on {Cued} {Recall} in {Memory}-{Impaired} {Individuals} with {Neurological} {Damage}},
	volume = {17},
	issn = {1469-7661, 1355-6177},
	shorttitle = {The {Self}-{Imagination} {Effect}},
	url = {https://www.cambridge.org/core/journals/journal-of-the-international-neuropsychological-society/article/selfimagination-effect-benefits-of-a-selfreferential-encoding-strategy-on-cued-recall-in-memoryimpaired-individuals-with-neurological-damage/0C2D2893E7F13B58C8FE86760F540D80},
	doi = {10.1017/S1355617711000737},
	abstract = {Knowledge of oneself is preserved in many memory-impaired individuals with neurological damage. Therefore, cognitive strategies that capitalize on mechanisms related to the self may be particularly effective at enhancing memory in this population. The present study investigated the effect of “self-imagining,” imagining an event from a personal perspective, on short and long delayed cued recall in memory-impaired individuals with neurological damage. Sixteen patients intentionally encoded word pairs under four separate conditions: visual imagery, semantic elaboration, other person imagining, and self-imagining. The results revealed that self-imagining led to better performance than other-imagining, semantic elaboration, and visual imagery. Furthermore, the “self-imagination effect” (SIE) was preserved after a 30-min delay and was independent of memory functioning. These findings indicate that self-imagining provides a mnemonic advantage in brain-injured individuals, even those with relatively poor memory functioning, and suggest that self-imagining may tap into mnemonic mechanisms related to the self. (JINS, 2011, 17, 929–933)},
	language = {en},
	number = {5},
	urldate = {2023-04-19},
	journal = {Journal of the International Neuropsychological Society},
	author = {Grilli, Matthew D. and Glisky, Elizabeth L.},
	month = sep,
	year = {2011},
	note = {Publisher: Cambridge University Press},
	keywords = {Self, Brain injury, Episodic memory, Imagination, Memory disorders, Memory rehabilitation},
	pages = {929--933},
}

